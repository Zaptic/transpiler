    "description": "A simple ts type transpiler",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "Ben <ben@zaptic.com>",
    "license": "MIT",
    "private": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Zaptic/transpiler"
